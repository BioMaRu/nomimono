{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/nomimono-react/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nrwl/web:rollup",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/nomimono-react",
				"tsConfig": "packages/nomimono-react/tsconfig.lib.json",
				"project": "packages/nomimono-react/package.json",
				"entryFile": "packages/nomimono-react/src/index.ts",
				"external": ["react/jsx-runtime"],
				"rollupConfig": "@nrwl/react/plugins/bundle-rollup",
				"compiler": "babel",
				"assets": [
					{
						"glob": "packages/nomimono-react/README.md",
						"input": ".",
						"output": "."
					}
				]
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"packages/nomimono-react/**/*.{ts,tsx,js,jsx}"
				]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/packages/nomimono-react"],
			"options": {
				"jestConfig": "packages/nomimono-react/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"storybook": {
			"executor": "@nrwl/storybook:storybook",
			"options": {
				"uiFramework": "@storybook/react",
				"port": 4400,
				"staticDir": ["packages/nomimono-react/assets"],
				"config": {
					"configFolder": "packages/nomimono-react/.storybook"
				}
			},
			"extractCss": true,
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"build-storybook": {
			"executor": "@nrwl/storybook:build",
			"outputs": ["{options.outputPath}"],
			"options": {
				"uiFramework": "@storybook/react",
				"outputPath": "dist/storybook/nomimono-react",
				"staticDir": ["packages/nomimono-react/assets"],
				"config": {
					"configFolder": "packages/nomimono-react/.storybook"
				}
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"version": {
			"executor": "@jscutlery/semver:version",
			"options": {
				"preset": "conventional"
			}
		}
	}
}

{
	"extends": "nx/presets/core.json",
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"npmScope": "nomimono",
	"affected": {
		"defaultBase": "main"
	},
	"cli": {
		"defaultCollection": "@nrwl/react"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e", "build-storybook"]
			}
		}
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"babel": true
			}
		}
	}
}
